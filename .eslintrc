{
  "extends": [
    "eslint:recommended",
    "plugin:unicorn/recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:svelte/recommended"
  ],
  "overrides": [
    {
      "files": ["*.svelte"],
      "parser": "svelte-eslint-parser",
      "parserOptions": { "parser": { "ts": "@typescript-eslint/parser" } }
    }
  ],
  "rules": {
    "unicorn/no-useless-undefined": "off"
  }
}
